{
  "name": "Netflix-Stop-Autoplay",
  "version": "2.2",
  "description": "An open source project that stops Netflix from auto-playing videos!",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.netflix.com/*"],
      "js": ["dist/content.js"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
